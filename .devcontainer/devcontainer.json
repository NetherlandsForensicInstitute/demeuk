{
    "name": "Demeuk devcontainer",
    "build":{
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/app/src,type=bind",
    "workspaceFolder": "/app/src",

    "customizations": {
    // Configure properties specific to VS Code.

        "vscode": {
            "extensions": [
                "ms-python.python"
            ]
        }
    },
    "remoteUser": "vscode"
}